<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="tpimport_view_company_form" model="ir.ui.view">
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/notebook/page/group/group[2]/field[@name='company_registry']" position="after">
                <field name="iban" string="IBAN" />
                <field name="fax" string="Fax" />
                <field name="cell" string="Cell"/>
            </xpath>
        </field>
    </record>
</odoo>